        "@types/redux-logger": "^3.0.7",
        "@types/styled-components": "^4.1.19",
        "babel-jest": "24.9.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "eslint": "^5.0.0",
        "jest": "24.9.0",
        "metro-react-native-babel-preset": "^0.54.1",
        "react-native-dotenv": "^0.2.0",
        "react-test-renderer": "16.8.6",
        "redux-mock-store": "^1.5.3",
        "typescript": "3.6.2"
    },
    "jest": {
        "preset": "react-native"
    }
